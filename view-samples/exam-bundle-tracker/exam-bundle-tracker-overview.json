    "children": [
      {
        "elmType": "div",
        "style": {
          "text-align": "left",
          "border-left": "0px",
          "padding-left": "2px",
          "width": "100%"
        },
        "children": [
          {
            "elmType": "div",
            "style": {
              "flex-wrap": "wrap",
              "display": "flex",
              "margin-left": "5px"
            },
            "attributes": {
              "class": "sp-row-listPadding"
            },
            "children": [
              {
                "elmType": "span",
                "style": {
                  "box-sizing": "border-box",
                  "padding": "4px 8px 5px 8px",
                  "overflow": "hidden",
                  "text-overflow": "ellipsis",
                  "display": "flex",
                  "border-radius": "5px",
                  "height": "24px",
                  "align-items": "center",
                  "white-space": "nowrap",
                  "margin": "4px 4px 4px 4px"
                },
                "attributes": {
                  "class": "sp-field-fontSizeSmall sp-css-backgroundColor-neutralBackground sp-css-color-neutralFont"
                },
                "children": [
                  {
                    "elmType": "span",
                    "attributes": {
                      "iconName": "Calendar"
                    },
                    "style": {
                      "font-size": "medium"
                    }
                  },
                  {
                    "elmType": "span",
                    "txtContent": "[$Yearoftask]",
                    "style": {
                      "margin-left": "4px"
                    },
                    "attributes": {
                      "title": "Academic year of this task"
                    }
                  }
                ]
              },
              {
                "elmType": "span",
                "style": {
                  "box-sizing": "border-box",
                  "padding": "4px 8px 5px 8px",
                  "overflow": "hidden",
                  "text-overflow": "ellipsis",
                  "display": "flex",
                  "border-radius": "5px",
                  "height": "24px",
                  "align-items": "center",
                  "white-space": "nowrap",
                  "margin": "4px 4px 4px 4px"
                },
                "attributes": {
                  "class": "sp-field-fontSizeSmall sp-css-backgroundColor-disabledBackground",
                  "title": "Cohort/Course which this task applies to"
                },
                "txtContent": "='Year ' + [$Course_x002f_Year]"
              },
              {
                "elmType": "span",
                "style": {
                  "box-sizing": "border-box",
                  "padding": "4px 8px 5px 8px",
                  "overflow": "hidden",
                  "text-overflow": "ellipsis",
                  "display": "flex",
                  "border-radius": "5px",
                  "height": "24px",
                  "align-items": "center",
                  "white-space": "nowrap",
                  "margin": "4px 4px 4px 4px"
                },
                "attributes": {
                  "class": "sp-field-fontSizeSmall sp-css-backgroundColor-BgLightBlue20",
                  "title": "Task Number"
                },
                "txtContent": "='Assessment Task ' + [$TaskNumber]"
              },
              {
                "elmType": "span",
                "style": {
                  "box-sizing": "border-box",
                  "padding": "4px 8px 5px 8px",
                  "overflow": "hidden",
                  "text-overflow": "ellipsis",
                  "display": "flex",
                  "border-radius": "5px",
                  "height": "24px",
                  "align-items": "center",
                  "white-space": "nowrap",
                  "margin": "4px 4px 4px 4px"
                },
                "attributes": {
                  "class": "sp-field-fontSizeSmall sp-css-backgroundColor-BgDarkPink sp-css-color-WhiteText",
                  "title": "Bundle number"
                },
                "children": [
                  {
                    "elmType": "span",
                    "attributes": {
                      "iconName": "TimeEntry"
                    },
                    "style": {
                      "font-size": "medium"
                    }
                  },
                  {
                    "elmType": "span",
                    "txtContent": "='Bundle number: ' + [$BundleNumber]",
                    "style": {
                      "margin-left": "4px"
                    }
                  }
                ]
              }
            ]
          },
          {
            "elmType": "div",
            "attributes": {
              "class": "sp-row-title",
              "title": "Bundle information"
            },
            "style": {
              "font-weight": "bold",
              "font-size": "xx-large",
              "margin-left": "10px",
              "margin-top": "10px",
              "display": "flex",
              "flex-wrap": "wrap",
              "vertical-align": "middle"
            },
            "children": [
              {
                "elmType": "div",
                "attributes": {
                  "iconName": "=if([$Incirculation] == false, 'CompletedSolid','IssueTracking' )",
                  "title": "Bundle information"
                },
                "style": {
                  "vertical-align": "middle",
                  "font-size": "45px"
                }
              },
              {
                "elmType": "div",
                "txtContent": "='Bundle ' + [$BundleNumber] + ' | ' + length([$Marked]) + ' of ' + toString( length([$NotMarked]) + length([$Marked]) + ' markers completed' + if([$Incirculation] == false,' (Task now archived)','')",
                "style": {
                  "margin-left": "8px",
                  "vertical-align": "middle",
                  "text-decoration": "=if([$Incirculation] == false ,'line-through red 3px','none')"
                },
                "attributes": {
                  "title": "Bundle information"
                }
              }
            ]
          },
          {
            "elmType": "div",
            "style": {
              "display": "flex",
              "width": "100%",
              "border-top-width": "0px",
              "border-top-style": "solid"
            },
            "attributes": {
              "class": "sp-row-listPadding sp-css-borderRight-warningBorder"
            },
            "children": [
              {
                "elmType": "div",
                "style": {
                  "width": "24%",
                  "border-right-width": "0px",
                  "border-right-style": "solid",
                  "padding-right": "20px"
                },
                "attributes": {
                  "class": "sp-row-listPadding sp-css-borderRight-warningBorder"
                },
                "children": [
                  {
                    "elmType": "div",
                    "style": {
                      "font-weight": "bold",
                      "font-size": "medium",
                      "display": "flex",
                      "flex-wrap": "wrap",
                      "margin-left": "10px",
                      "border-top-width": "0px",
                      "border-top-style": "solid"
                    },
                    "attributes": {
                      "class": "sp-css-borderTop-warningBorder"
                    },
                    "txtContent": "=if(length([$Inpossession]) != 0, 'In possession','Bundle location')"
                  },
                  {
                    "elmType": "div",
                    "style": {
                      "display": "flex",
                      "height": "100px"
                    },
                    "children": [
                      {
                        "elmType": "div",
                        "style": {
                          "display": "=if(length([$Inpossession]) != 0, 'none','flex')",
                          "margin-left": "10px"
                        },
                        "children": [
                          {
                            "elmType": "div",
                            "style": {
                              "font-size": "large"
                            },
                            "attributes": {
                              "iconName": "CompletedSolid",
                              "class": "sp-css-color-MintGreenFont"
                            }
                          },
                          {
                            "elmType": "div",
                            "style": {
                              "margin-left": "10px",
                              "width": "100%",
                              "font-size": "larger",
                              "display": "flex"
                            },
                            "txtContent": "Inside cupboard"
                          }
                        ]
                      },
                      {
                        "elmType": "div",
                        "style": {
                          "display": "flex",
                          "flex-wrap": "wrap",
                          "height": "28px"
                        },
                        "children": [
                          {
                            "elmType": "div",
                            "forEach": "person in [$Inpossession]",
                            "style": {
                              "display": "=if(length([$Inpossession]) != 0 ,'flex','none')",
                              "flex-direction": "row",
                              "align-items": "center",
                              "white-space": "nowrap",
                              "border-radius": "13px",
                              "margin-left": "10px",
                              "margin-top": "5px"
                            },
                            "attributes": {
                              "class": "ms-bgColor-neutralLight"
                            },
                            "defaultHoverField": "[$person]",
                            "children": [
                              {
                                "elmType": "img",
                                "style": {
                                  "width": "28px",
                                  "height": "28px",
                                  "border-radius": "50%"
                                },
                                "attributes": {
                                  "src": "=getUserImage([$person.email], 'small')"
                                }
                              },
                              {
                                "elmType": "div",
                                "txtContent": "[$person.title]",
                                "style": {
                                  "padding-left": "6px",
                                  "padding-right": "10px"
                                },
                                "attributes": {
                                  "class": "ms-fontColor-neutralPrimary"
                                }
                              }
                            ]
                          }
                        ]
                      }
                    ]
                  },
                  {
                    "elmType": "div",
                    "style": {
                      "display": "flex"
                    },
                    "children": [
                      {
                        "elmType": "button",
                        "customRowAction": {
                          "action": "setValue",
                          "actionInput": {
                            "Inpossession": "=if(indexOf([$Inpossession.email], @me) == -1, @me, '')",
                            "Updatelog": "=if([$Inpossession.email],'📥 Bundle has been returned to the cupboard','📤 Bundle is in possession and being marked')"
                          }
                        },
                        "attributes": {
                          "class": "='sp-row-button ms-fontWeight-semibold ms-fontColor-black ' + if(indexOf([$Inpossession.email] , @me) == -1, if(indexOf([$Course_x002f_Year],'Ext 1') == -1, 'sp-css-backgroundColor-BgMintGreen  sp-css-backgroundColor-BgMintGreen30--hover', 'sp-css-backgroundColor-BgCyan  sp-css-backgroundColor-BgCyan30--hover'),'ms-bgColor-grey ms-bgColor-neutralQuaternary--hover ms-fontColor-white--hover')"
                        },
                        "style": {
                          "width": "250px",
                          "margin-left": "10px",
                          "margin-top": "8px",
                          "display": "span"
                        },
                        "children": [
                          {
                            "elmType": "span",
                            "attributes": {
                              "iconName": "=if(indexOf([$Inpossession.email] , @me) == -1,'PageCheckedout','PageCheckedin')",
                              "class": "=if(indexOf([$Inpossession.email] , @me) == -1,'','sp-css-color-MintGreenFont')"
                            },
                            "style": {
                              "padding-right": "4px",
                              "font-size": "large",
                              "vertical-align": "text-bottom"
                            }
                          },
                          {
                            "elmType": "span",
                            "txtContent": "=if(indexOf([$Inpossession.email] , @me) == -1,'Take possession','Bundle  in possession')",
                            "style": {
                              "word-break": "keep-all"
                            },
                            "attributes": {
                              "class": "=if(indexOf([$Inpossession.email] , @me) == -1,'','sp-css-color-MintGreenFont')"
                            }
                          }
                        ]
                      }
                    ]
                  }
                ]
              },
              {
                "elmType": "div",
                "style": {
                  "width": "38%",
                  "float": "left",
                  "border-left-width": "2px",
                  "border-left-style": "dashed",
                  "padding-left": "10px"
                },
                "attributes": {
                  "class": "=if(indexOf([$Course_x002f_Year], 'Adv') != -1,'sp-css-backgroundColor-BgPeach sp-field-borderLeftBold sp-field-borderLeftSolid sp-css-borderLeftColor-PeachFont',if(indexOf([$Course_x002f_Year], 'Ext 1') != -1,'sp-css-backgroundColor-BgMintGreen sp-field-borderLeftBold sp-field-borderLeftSolid sp-css-borderLeftColor-MintGreenFont',if(indexOf([$Course_x002f_Year], 'Ext 2') != -1, 'sp-css-backgroundColor-BgCornflowerBlue  sp-field-borderLeftBold sp-field-borderLeftSolid sp-css-borderLeftColor-CornflowerBlueFont',if(indexOf([$Course_x002f_Year], '10') != -1,'sp-css-backgroundColor-BgCyan sp-field-borderLeftBold sp-field-borderLeftSolid sp-css-borderLeftColor-CyanFont',if(indexOf([$Course_x002f_Year], '09') != -1,'sp-css-backgroundColor-BgDustRose sp-field-borderLeftBold sp-field-borderLeftSolid sp-css-borderLeftColor-DustRoseFont',if(indexOf([$Course_x002f_Year], '08') != -1,'sp-css-backgroundColor-BgCoral sp-field-borderLeftBold sp-field-borderLeftSolid sp-css-borderLeftColor-CoralFont','sp-css-backgroundColor-BgGold sp-field-borderLeftBold sp-field-borderLeftSolid sp-css-borderLeftColor-GoldFont'))))))"
                },
                "children": [
                  {
                    "elmType": "div",
                    "style": {
                      "font-weight": "bold",
                      "font-size": "medium",
                      "display": "flex",
                      "flex-wrap": "wrap",
                      "margin-left": "10px",
                      "border-top-width": "0px",
                      "border-top-style": "solid"
                    },
                    "attributes": {
                      "class": "sp-css-borderTop-warningBorder"
                    },
                    "txtContent": "Not marked"
                  },
                  {
                    "elmType": "div",
                    "style": {
                      "display": "flex",
                      "height": "100px"
                    },
                    "children": [
                      {
                        "elmType": "div",
                        "style": {
                          "display": "flex",
                          "flex-wrap": "wrap",
                          "height": "28px"
                        },
                        "children": [
                          {
                            "elmType": "div",
                            "forEach": "person in [$NotMarked]",
                            "style": {
                              "display": "=if([$NotMarked],'flex','none')",
                              "flex-direction": "row",
                              "align-items": "center",
                              "white-space": "nowrap",
                              "border-radius": "13px",
                              "margin": "4px",
                              "margin-left": "10px"
                            },
                            "attributes": {
                              "class": "ms-bgColor-neutralLight"
                            },
                            "defaultHoverField": "[$person]",
                            "children": [
                              {
                                "elmType": "img",
                                "style": {
                                  "width": "28px",
                                  "height": "28px",
                                  "border-radius": "50%"
                                },
                                "attributes": {
                                  "src": "=getUserImage([$person.email], 'small')"
                                }
                              },
                              {
                                "elmType": "div",
                                "txtContent": "[$person.title]",
                                "style": {
                                  "padding-left": "6px",
                                  "padding-right": "10px"
                                },
                                "attributes": {
                                  "class": "ms-fontColor-neutralPrimary"
                                }
                              }
                            ]
                          }
                        ]
                      }
                    ]
                  },
                  {
                    "elmType": "div",
                    "style": {
                      "display": "flex"
                    },
                    "children": [
                      {
                        "elmType": "button",
                        "customRowAction": {
                          "action": "setValue",
                          "actionInput": {
                            "NotMarked": "=if(indexOf([$NotMarked.email] , @me) > -1, removeFrom([$NotMarked.email] , @me), appendTo([$NotMarked.email] , @me))",
                            "Marked": "=if(indexOf([$Marked.email] , @me) > -1, removeFrom([$Marked.email] , @me), appendTo([$Marked.email] , @me))",
                            "Updatelog": "=if(indexOf([$NotMarked.email] , @me) > -1 ,'✅📝 This bundle has been marked','❌📝 This bundle not yet fully marked')"
                          }
                        },
                        "attributes": {
                          "class": "='sp-row-button ms-fontWeight-semibold ms-fontColor-black ' + if(indexOf([$NotMarked.email] , @me) != -1, if(indexOf([$Course_x002f_Year],'Ext 1') == -1, 'sp-css-backgroundColor-BgMintGreen  sp-css-backgroundColor-BgMintGreen30--hover', 'sp-css-backgroundColor-BgCyan  sp-css-backgroundColor-BgCyan30--hover'),'ms-bgColor-grey ms-bgColor-neutralQuaternary--hover ms-fontColor-white--hover')"
                        },
                        "style": {
                          "width": "250px",
                          "margin-left": "10px",
                          "margin-top": "8px",
                          "display": "span"
                        },
                        "children": [
                          {
                            "elmType": "span",
                            "attributes": {
                              "iconName": "PenWorkspace"
                            },
                            "style": {
                              "padding-right": "4px",
                              "font-size": "large",
                              "vertical-align": "text-bottom"
                            }
                          },
                          {
                            "elmType": "span",
                            "txtContent": "=if(indexOf([$NotMarked.email] , @me) != -1, 'Click to set as Marked', 'Click to set as Unmarked')",
                            "style": {
                              "word-break": "keep-all"
                            }
                          }
                        ]
                      }
                    ]
                  }
                ]
              },
              {
                "elmType": "div",
                "style": {
                  "width": "38%",
                  "float": "left",
                  "border-right-width": "0px",
                  "border-right-style": "solid"
                },
                "attributes": {
                  "class": "sp-row-listPadding sp-css-borderRight-warningBorder"
                },
                "children": [
                  {
                    "elmType": "div",
                    "style": {
                      "font-weight": "bold",
                      "font-size": "medium",
                      "display": "flex",
                      "flex-wrap": "wrap",
                      "margin-left": "10px",
                      "border-top-width": "0px",
                      "border-top-style": "solid"
                    },
                    "attributes": {
                      "class": "sp-css-borderTop-warningBorder"
                    },
                    "txtContent": "Marked"
                  },
                  {
                    "elmType": "div",
                    "style": {
                      "display": "flex",
                      "height": "145px"
                    },
                    "children": [
                      {
                        "elmType": "div",
                        "style": {
                          "display": "flex",
                          "flex-wrap": "wrap",
                          "height": "28px"
                        },
                        "children": [
                          {
                            "elmType": "div",
                            "style": {
                              "display": "flex",
                              "flex-wrap": "wrap"
                            },
                            "children": [
                              {
                                "elmType": "div",
                                "forEach": "person in [$Marked]",
                                "style": {
                                  "display": "=if([$Marked],'flex','none')",
                                  "flex-direction": "row",
                                  "align-items": "center",
                                  "white-space": "nowrap",
                                  "border-radius": "13px",
                                  "margin": "4px",
                                  "margin-left": "10px"
                                },
                                "attributes": {
                                  "class": "ms-bgColor-neutralLight"
                                },
                                "defaultHoverField": "[$person]",
                                "children": [
                                  {
                                    "elmType": "img",
                                    "style": {
                                      "width": "28px",
                                      "height": "28px",
                                      "border-radius": "50%"
                                    },
                                    "attributes": {
                                      "src": "=getUserImage([$person.email], 'small')"
                                    }
                                  },
                                  {
                                    "elmType": "div",
                                    "txtContent": "[$person.title]",
                                    "style": {
                                      "padding-left": "6px",
                                      "padding-right": "10px"
                                    },
                                    "attributes": {
                                      "class": "ms-fontColor-neutralPrimary"
                                    }
                                  }
                                ]
                              }
                            ]
                          }
                        ]
                      }
                    ]
                  }
                ]
              }
            ]
          },
          {
            "elmType": "div",
            "style": {
              "display": "block",
              "margin-top": "0px"
            },
            "children": [
              {
                "elmType": "div",
                "style": {
                  "display": "block",
                  "float": "none",
                  "border-top-width": "0px",
                  "border-top-style": "solid"
                },
                "attributes": {
                  "class": "sp-row-listPadding sp-css-borderTop-successBorder"
                },
                "children": [
                  {
                    "elmType": "div",
                    "style": {
                      "display": "inline-flex",
                      "vertical-align": "middle",
                      "font-size": "large",
                      "margin-left": "10px"
                    },
                    "attributes": {
                      "iconName": "SetAction"
                    }
                  },
                  {
                    "elmType": "div",
                    "style": {
                      "font-weight": "bold",
                      "font-size": "medium",
                      "display": "inline-flex",
                      "margin-top": "0px",
                      "margin-left": "10px",
                      "border-top-width": "0px",
                      "border-top-style": "solid"
                    },
                    "attributes": {
                      "class": "='sp-row-listPadding ' + if([$TopicContentSequence] == 0 , 'sp-css-borderTop-warningBorder' 'sp-css-borderTop-successBorder')"
                    },
                    "txtContent": "Actions"
                  },
                  {
                    "elmType": "div",
                    "style": {
                      "display": "flex"
                    },
                    "children": [
                      {
                        "elmType": "button",
                        "customRowAction": {
                          "action": "setValue",
                          "actionInput": {
                            "Incirculation": "=if([$Incirculation],0, 1)"
                          }
                        },
                        "attributes": {
                          "class": "='sp-row-button ' + if([$TaskActive] && length([$LatestQuestionPaper]) != 0 && length([$LatestSolutions]) != 0, if(indexOf([$Course_x002f_Year],'Ext 1') == -1, 'sp-css-backgroundColor-BgMintGreen  sp-css-backgroundColor-BgMintGreen30--hover', 'sp-css-backgroundColor-BgCyan  sp-css-backgroundColor-BgCyan30--hover'),'ms-bgColor-grey ms-bgColor-neutralQuaternary--hover ms-fontColor-white--hover')"
                        },
                        "style": {
                          "width": "250px",
                          "margin-left": "10px",
                          "display": "span"
                        },
                        "children": [
                          {
                            "elmType": "span",
                            "attributes": {
                              "iconName": "=if([$Incirculation] == true, 'CompletedSolid', 'ArchiveUndo')",
                              "class": "=if([$Incirculation] == true, 'sp-css-color-MintGreenFont', 'sp-css-color-DustRoseFont')"
                            },
                            "style": {
                              "padding-right": "4px",
                              "font-size": "large",
                              "vertical-align": "text-bottom"
                            }
                          },
                          {
                            "elmType": "span",
                            "txtContent": "=if([$Incirculation] == true, 'Click to set bundle as fully marked','Click to set bundle as unmarked')",
                            "style": {
                              "word-break": "keep-all"
                            },
                            "attributes": {
                              "class": "=if([$Incirculation] == true, 'sp-css-color-MintGreenFont','sp-css-color-DustRoseFont')"
                            }
                          }
                        ]
                      },
                      {
                        "elmType": "button",
                        "customRowAction": {
                          "action": "share"
                        },
                        "style": {
                          "width": "250px",
                          "margin-left": "10px",
                          "display": "span"
